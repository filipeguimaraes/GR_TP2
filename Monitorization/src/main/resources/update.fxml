      <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="196.0" prefWidth="296.0">
         <children>
            <Region layoutX="10.0" layoutY="110.0" prefHeight="200.0" prefWidth="250.0" VBox.vgrow="ALWAYS" />
            <Label styleClass="control-label" text="Polling Time" />
            <HBox>
               <children>
                  <TextField fx:id="time" promptText="time" text="60" />
                  <Region prefHeight="26.0" prefWidth="44.0" HBox.hgrow="ALWAYS" />
                  <Button fx:id="apply" mnemonicParsing="false" onAction="#applyTime" prefHeight="26.0" prefWidth="106.0" styleClass="button-raised" text="Apply" />
               </children>
            </HBox>
            <Region layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="250.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <Region layoutX="520.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
   </children>
